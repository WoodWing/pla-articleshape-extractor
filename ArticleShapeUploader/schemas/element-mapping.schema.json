{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://www.woodwing.com/pla/element-mapping.schema.json",
    "title": "Custom (LHS) to standard (RHS) element label mapping table",
    "type": "object",
    "propertyNames": {
        "type": "string",
        "minLength": 1
    },
    "additionalProperties": {
        "type": "string",
        "enum": [
            "body",
            "breakout",
            "byline",
            "caption",
            "credit",
            "crosshead",
            "head",
            "highlight",
            "intro",
            "quote",
            "subhead"
        ]
    }
}